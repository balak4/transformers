{
    "dataset": {
        "name": "w601sxs/simpleCoT",
        "train": "train[:6000]",
        "valid": "train[6000:6100]",
        "test": "train[6100:6200]"
    },
    "base_train_args": {
        "per_device_train_batch_size": 3,
        "per_device_eval_batch_size": 1,
        "logging_steps": 2,
        "num_train_epochs": 1,
        "learning_rate": 0.0002,
        "bf16": "True",
        "save_strategy": "no",
        "report_to": "tensorboard"
    },
    "peft_ranges": {
        "r_alpha_values": [
            8
        ]
    },
    "schedulers": "greedy$min_lr:1.85e-05,smooth:True,factor:0.99"
}
